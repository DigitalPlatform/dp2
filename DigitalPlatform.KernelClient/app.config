<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_KernelService">
                    <security mode="None" />
                </binding>
            </netNamedPipeBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_KernelService" messageEncoding="Mtom">
                    <security>
                        <message clientCredentialType="None" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="net.pipe://localhost/dp2kernel/" binding="netNamedPipeBinding"
                bindingConfiguration="NetNamedPipeBinding_KernelService" contract="KernelServiceReference.KernelService"
                name="NetNamedPipeBinding_KernelService" />
            <endpoint address="http://localhost:8001/dp2kernel" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_KernelService" contract="KernelServiceReference.KernelService"
                name="WSHttpBinding_KernelService">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>